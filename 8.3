//统计num二进制中1的个数，补码中1的个数
int main()
{
	int num = 0;
	int count = 0;
	scanf("%d", &num);
	int i = 0;
	for(i=0;i<32;i++)
	{
		if (1==((num >>i)&1))
		{
			count++;
		}
	}
	printf("%d\n", count);
	return 0;
}
